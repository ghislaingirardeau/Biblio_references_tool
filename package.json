{
  "name": "my-biblio-tool",
  "version": "0.0.1",
  "description": "Build quickly your bibliography and add quotes to your references",
  "productName": "biblio_tool",
  "author": "ghislaingirardeau <g.girardeaupro@sfr.fr>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:pwa": "quasar dev -m pwa",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "postinstall": "quasar prepare",
    "start": "node server/index.js",
    "start:dev": "node --watch server/index.js"

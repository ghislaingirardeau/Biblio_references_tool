    "@vueuse/core": "^13.5.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "firebase": "^12.3.0",
    "pinia": "^3.0.1",
    "postcss": "^8.5.6",
    "quasar": "^2.16.0",
    "register-service-worker": "^1.7.2",
    "tesseract.js": "^6.0.1",
    "vue": "^3.4.18",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
